<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Sustainability.LandFillRate" table="landfillrate" catalog="gdp">
        <id name="landfilldate" type="timestamp">
            <column name="LandfillDate" length="20" />
            <generator class="assigned" />
        </id>
        <property name="rate" type="double">
            <column name="LandfillRate" length="45"/>
        </property>
    </class>
</hibernate-mapping>
